<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings xmlns="https://jakarta.ee/xml/ns/persistence/orm"
                 version="3.0">

    <entity class="vn.edu.iuh.fit.backend.models.CustomerEntity">
        <table name="customer" schema="week2" catalog=""/>
        <attributes>
            <id name="custId">
                <column name="cust_id"/>
            </id>
            <basic name="custName">
                <column name="cust_name"/>
            </basic>
            <basic name="email">
                <column name="email"/>
            </basic>
            <basic name="phone">
                <column name="phone"/>
            </basic>
            <basic name="address">
                <column name="address"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
